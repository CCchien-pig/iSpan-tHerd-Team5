{
  "name": "flexfrontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,vue,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,vue,json,css,scss,md}\"",
    "format:all": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,json,css,scss,md}\" --ignore-path .prettierignore",
    "ngrok:frontend": "ngrok http 5173",
    "ngrok:backend": "ngrok http 7200 --host-header=\"localhost:7200\"",
    "dev:with-ngrok": "run-p dev ngrok:frontend"
  },
  "dependencies": {
    "@fawmi/vue-google-maps": "^0.9.79",
    "axios": "^1.12.2",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "chart.js": "^4.5.1",
    "chartjs-plugin-annotation": "^3.1.0",
    "echarts": "^6.0.0",
    "element-plus": "^2.11.5",
    "node-vibrant": "^4.0.3",
    "pinia": "^3.0.3",
    "qrcode": "^1.5.4",
    "sweetalert2": "^11.26.3",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vue3-treeselect": "^0.1.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@prettier/plugin-oxc": "^0.0.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.33.0",
    "eslint-plugin-oxlint": "~1.11.0",
    "eslint-plugin-vue": "~10.4.0",
    "globals": "^16.3.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.11.0",
    "prettier": "^3.6.2",
    "vite": "^7.1.9",
    "vite-plugin-vue-devtools": "^8.0.2"
  }
}
